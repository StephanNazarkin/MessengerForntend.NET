@{
    Layout = null;
}
<div class="modal-header">
    <h5 class="modal-title" id="blackListModalLabel">Black List</h5>
    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
</div>
<div class="modal-body">
    @foreach (var friend in ViewBag.AllBlockedUsers)
            {
                <div class="d-flex justify-content-between align-items-center border-top border-bottom p-1">
                    <p class="m-0">
                        @friend.userName
                    </p>
                    <button type="button" class="btn btn-primary" style="float: right;">Remove</button>
                </div>
            }
</div>
<div class="modal-footer">
    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
</div>